# Bot Configuration
PREFIX=""
SESSION_ID="your-bot"
PHONE_NUMBER="62xxx"
OWNER_NUMBER="62xxx"

# Features Configuration
USE_STORE=false

# Command Configuration
COMMAND_COOLDOWN=3

# Database Configuration
DATABASE_URI="sqlite:bot.db"

# Media Configuration
MAX_FILE_SIZE=100000000 # dalam bytes (100MB)
ALLOWED_MEDIA_TYPES="image,video,document,audio,sticker"

# Security Configuration
ENABLE_ANTI_SPAM=true
SPAM_THRESHOLD=5
SPAM_TIMEOUT=60000 # dalam milidetik (60 detik)

# Group Configuration
MAX_GROUP_SIZE=256
AUTO_READ_MESSAGES=true
AUTO_PRESENCE_UPDATE=true

# Debug Configuration
DEBUG_MODE=false
LOG_LEVEL="info" # debug, info, warn, error 